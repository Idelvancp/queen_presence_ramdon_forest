<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package= "com.example.queen_presence_ramdon_forest">
    <!-- The INTERNET permission is required for development. Specifically,
         the Flutter tool needs it to communicate with the running application
         to allow setting breakpoints, to provide hot reload, etc.
    -->


    <application>
        android:label="MyApp Profile"
        android:debuggable="true" <!-- Necessário para o modo profile -->
        android:usesCleartextTraffic="true"> <!-- Permitir tráfego HTTP, se necessário para debugging -->
    <!-- Outras configurações -->
        <profileable android:shell="true" />
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <!-- Permissões opcionais para registro de métricas -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
</manifest>
